return ;
}
try{
var source = document.InputForm.input_file_fake_input_file.value.split( ", " );
var fs = new ActiveXObject( "Scripting.FileSystemObject" );
var FileShort = '"' + fs.GetFile( source[0] ).ShortPath + '"';
var TheFiles  = FileShort + s;

for( i=1; i<source.length; i++){
var FileShort = fs.GetFile( source[i] ).ShortPath;
var TheFiles  = TheFiles + s + FileShort;
//alert( source[i] );
}
}catch(e){
var source = document.InputForm.input_file_fake_input_file.value;
