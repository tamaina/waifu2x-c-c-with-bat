form.otheropco01.value                                 = splited[19].substr( 1 , splited[19].length - 2 ).replace( /△/gi ," " );
form.otheropsz01.value                                 = splited[20].substr( 1 , splited[20].length - 2 ).replace( /△/gi ," " );
form.otheropff01.value                                 = splited[21].substr( 1 , splited[21].length - 2 ).replace( /△/gi ," " );

if ( splited[22] == "a" ) {
form.TMPFolderMode[0].checked                = true;
